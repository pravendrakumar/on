* {margin:0; padding:0; outline:0;  box-sizing: border-box;  -ms-box-sizing: -webkit-border-box;  -moz-box-sizing: border-box;  -o-box-sizing: border-box;}
html, body {width:100%; height:100%;}
body {  font-family: 'robotoregular'; font-size:15px; line-height:normal; color:#818181;}
a {outline:none; text-decoration:none; color:#818181; transition:all 0.5s ease;}
input, textarea, select {font-size:13px; color:#3a3a3a; vertical-align:middle;  font-family:'robotomedium'; outline:none;}
input[type="button"] {-webkit-transition:ease-in-out .5s; -moz-transition:ease-in-out .5s; -ms-transition:ease-in-out .5s; -o-transition:ease-in-out .5s; transition:ease-in-out .5s;}
.transition {-webkit-transition:ease-in-out .5s; -moz-transition:ease-in-out .5s; -ms-transition:ease-in-out .5s; -o-transition:ease-in-out .5s; transition:ease-in-out .5s;}
.clr {clear:both !important;}
.fl {float:left !important;}
.fr {float:right !important;}
.vAlign {vertical-align:middle !important;}
img {border:none; vertical-align:top; max-width:100%;}
h1, h2, h3, h4, h5, h6 {font-weight:normal;  font-family:'robotomedium';}
ul {list-style:none;}
.clearfix:before, .clearfix:after {content:''; display:block; height:0; overflow:hidden;}
.clearfix:after {clear:both;}
.container{ max-width:1234px; margin:0 auto; clear:both; padding:0 10px;}
.mainCon{ width:100%;}
/*.header{ background:#d03807; height:68px; position:fixed; top:0; width:100%; left:0; right:0; z-index:100; text-align:center;}*/
.header{background:#0a9293; height:68px; position:fixed; top:0; width:100%; left:0; right:0; z-index:100; text-align:center; color:#9fd4d5;}
.logo{ float:left; padding:12px 0; margin-top:25px;}
/* .serachCon{ height:33px; width:100%; margin:17px 0; max-width:700px; border:1px solid #f0592a; border-radius:5px; padding-right:60px; padding-left:40px; position:relative; float:left; margin-left:60px;} */
.serachCon{ height:33px; width:100%; margin:17px 0; max-width:700px; border:1px solid #9fd4d5; border-radius:5px; padding-right:60px; padding-left:40px; position:relative; float:left; margin-left:60px;}
.serachCon img{ float:left; padding:0px; position:absolute; left:10px; top:11px;}
/* .srchInput { background: transparent none repeat scroll 0 0;border: 0 none; color: #fd7d5a;font-size: 15px; font-weight: normal; height: 31px;width: 100%;} */
.srchInput { background: transparent none repeat scroll 0 0;border: 0 none; color: #9fd4d5;font-size: 15px; font-weight: normal; height: 31px;width: 100%;}
.srchBtn{ width:34px; background:url(../../img/Front/searchIcon.png) no-repeat right center; height:18px; border:0; position:absolute; right:10px; top:6px; cursor:pointer; border-left:1px solid #fff;}
.signIn{ float:right; margin-top:20px;}
.signIn a{ margin-left:10px;}
